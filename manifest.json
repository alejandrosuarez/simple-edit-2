  ],
  "categories": [
    "photo",
    "utilities"
  ],
  "shortcuts": [
    {
      "name": "Start Creating",
      "url": "/home",
      "description": "Jump straight to the editor",
      "icons": [{ "src": "/assets/icons_new/192x192-icon.png", "sizes": "192x192" }]
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "web+simpleedit",
      "url": "/?url=%s"
    }
  ],
  "share_target": {
    "action": "/share/image/",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "name",
      "text": "description",
      "files": [
        {
          "name": "file",
          "accept": [
            "image/jpeg",
            ".png",
            ".jpg",
            ".webp"
          ]
        }
      ]
    }
  },
  "file_handlers": [
    {
      "action": "/",
      "accept": {
        "image/*": [
          ".png",
          ".jpg",
          ".webp"
        ]
      }
    }
  ],
  "scope": "/"
}